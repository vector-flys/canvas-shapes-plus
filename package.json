{
  "name": "shapes-plus",
  "description": "Used canvas library to make shapes",
  "version": "0.1.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/mjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build-all": "rimraf dist && tsc -b ./tsconfig.cjs.json ./tsconfig.mjs.json ./tsconfig.types.json",
    "build": "rimraf dist && tsc -b ./tsconfig.mjs.json ./tsconfig.types.json"
  },
  "keywords": [
    "canvas",
    "form",
    "forms",
    "shapes",
    "shape",
    "plus"
  ],
  "author": "Hlever",
  "license": "ISC",
  "dependencies": {
    "canvas": "^3.0.0-rc2"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4"
  }
}
